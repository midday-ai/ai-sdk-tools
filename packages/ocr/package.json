    "ai",
    "ai-sdk",
    "ocr",
    "invoice",
    "receipt",
    "document-extraction",
    "mistral",
    "gemini"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/midday-ai/ai-sdk-tools.git",
    "directory": "packages/ocr"
  },
  "bugs": {
    "url": "https://github.com/midday-ai/ai-sdk-tools/issues"
  },
  "homepage": "https://github.com/midday-ai/ai-sdk-tools/tree/main/packages/ocr#readme",
  "type": "module",
  "exports": {
