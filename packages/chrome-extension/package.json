{
  "name": "@ai-sdk-tools/chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extension for AI SDK DevTools",
  "main": "index.js",
  "scripts": {
    "build": "bun run tsup && node copy-files.js",
    "dev": "bun run tsup --watch",
    "package": "bun run build && zip -r ai-sdk-devtools.zip dist/",
    "clean": "rm -rf dist/",
    "setup": "bun install && bun run build"
  },
  "keywords": [
    "ai",
    "devtools",
    "chrome-extension",
    "ai-sdk",
    "debugging"
  ],
  "author": "Pontus Abrahamsson",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9"
  },
  "dependencies": {
    "@ai-sdk-tools/devtools": "workspace:*",
    "@ai-sdk-tools/store": "workspace:*"
  }
}
