{
  "name": "@ai-sdk-tools/store",
  "version": "0.8.2",
  "description": "A drop-in replacement for @ai-sdk/react that automatically syncs chat state to Zustand stores",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "react",
    "zustand",
    "state-management",
    "chat",
    "llm",
    "vercel"
  ],
  "author": "Pontus Abrahamsson",
  "license": "MIT",
  "peerDependencies": {
    "@ai-sdk/react": ">=2.0.0",
    "react": ">=18.0.0",
    "zustand": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "react": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "zustand": "^5.0.8",
    "@ai-sdk/react": "^2.0.60"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/midday-ai/ai-sdk-tools.git"
  },
  "bugs": {
    "url": "https://github.com/midday-ai/ai-sdk-tools/issues"
  },
  "homepage": "https://github.com/midday-ai/ai-sdk-tools#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ai": "^5.0.68"
  }
}
